  "scripts": {
    "postinstall": "npm run frontend:postinstall",
    "start": "npm run frontend:start",
    "backend:test": "make test",
    "backend:start": "make run",
    "backend:start-local-db": "redis-server",
    "backend:start-db-client": "copyfiles -u 1 redis-client-config/local.json node_modules/redis-commander/config && redis-commander",
    "backend:heroku-local": "heroku local",
    "backend:type-check": "make type-check",
    "frontend:postinstall": "npm --prefix './lambda-dev' install",
    "frontend:build-lambda": "npm run --prefix './lambda-dev' build-lambda",
    "frontend:netlify-dev": "npm run frontend:build-lambda && netlify dev",
    "frontend:clean": "gatsby clean",
    "frontend:start": "npm run frontend:develop",
    "frontend:build": "npm run frontend:clean && gatsby build && npm run frontend:build-lambda",
    "frontend:develop": "npm run frontend:clean && gatsby develop",
    "frontend:test": "echo \"Error: no test specified\" && exit 1",
    "frontend:type-check": "tsc --pretty --noEmit --skipLibCheck",
    "format": "prettier --write \"gatsby-*.js\" \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx",
    "type-check": "npm run frontend:type-check && npm run backend:type-check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run frontend:type-check && npm run backend:type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "devDependencies": {
    "@graphql-tools/wrap": "^7.0.1",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
